    "@repo/core": "workspace:*",
    "@repo/schemas": "workspace:*",
    "ai": "^6.0.49",
    "hono": "^4.11.4",
    "zhipu-ai-provider": "^0.2.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@repo/tsconfig": "workspace:*",
    "@types/node": "^22.0.0",
    "tsx": "^4.21.0",
