{
  "name": "@repo/server",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "exports": {
    ".": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js"
    }
