{
  "name": "@stargazer/core",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js"
    },
    "./result": {
      "types": "./dist/result.d.ts",
      "import": "./dist/result.js"
    },
    "./severity": {
      "types": "./dist/severity.d.ts",
      "import": "./dist/severity.js"
    },
    "./strings": {
      "types": "./dist/strings.d.ts",
      "import": "./dist/strings.js"
    },
    "./format": {
      "types": "./dist/format.d.ts",
      "import": "./dist/format.js"
    },
    "./json": {
      "types": "./dist/json.d.ts",
      "import": "./dist/json.js"
    },
    "./review": {
      "types": "./dist/review/index.d.ts",
      "import": "./dist/review/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@stargazer/schemas": "workspace:*",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@stargazer/tsconfig": "workspace:*",
    "@types/node": "^25.2.0",
    "typescript": "^5.9.3"
  }
}
