{
  "name": "@repo/core",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./review": {
      "types": "./dist/review/index.d.ts",
      "import": "./dist/review/index.js"
    },
    "./review/browser": {
      "types": "./dist/review/browser.d.ts",
      "import": "./dist/review/browser.js"
    },
    "./diff": {
      "types": "./dist/diff/index.d.ts",
      "import": "./dist/diff/index.js"
    },
    "./ai": {
      "types": "./dist/ai/index.d.ts",
      "import": "./dist/ai/index.js"
    },
    "./testing": {
      "types": "./dist/__test__/testing.d.ts",
      "import": "./dist/__test__/testing.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "type-check-test": "tsc --noEmit --project tsconfig.test.json"
  },
  "sideEffects": false,
  "dependencies": {
    "@repo/schemas": "workspace:*",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@repo/tsconfig": "workspace:*",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}
